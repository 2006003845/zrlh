package com.zrong.Android.element;

public class Branch {
	
	public static final byte PERSONNAL=0;//人事部
	public static final byte MARKET=1;//公关部
	public static final byte PLANNING=2;//策划部
	public static final byte FINANCE=3;//财务部
	public static final byte SHOP=4;//店铺
	public static final byte BUILDING=5;//公关建筑
	/**部门id*/
	public long id;
	/**部门名称*/
	public String name;
	/**部门类型*/
	public byte type=-1;//0人事部1公关部2策划部3财务部4店铺5建筑6空地
	
	/**部门员工 */
	public Employee employees[] =  new Employee[0];
	/**部门人数 */
	public short staffNum;
	/**部门最大人数 */
	public short maxStaffNum;
}
