package com.zrong.Android.element;
/**
 * 
 * @author 公益建筑基本信息
 *
 */
public class PublicBuilding extends Building
{
	
	/**店长id*/
	public	long manager_id;
	/**店铺员工数*/
	public	short staffNum;
	/**店铺员工上限*/
	public short maxStaffNum;
	/**公益建筑人流增益*/
	public short flowGain;
	/**公益建筑规模*/
	public byte scale;
	/**店铺所在地图id*/
	public short map_id;
	/**店铺的左上格子x坐标*/
	public short address_x;
	/**店铺的左上格子y坐标*/
	public short address_y;
	/**店铺左上格的起始小格子位置（0-8）*/
	public byte address_i;
	/**店铺在地图上对应的建筑物id*/
	public long build_id;

	
