package com.zrong.Android.View;

import java.util.Hashtable;




public class Event {
	
	public Event(int action,Hashtable param)
	{
		setAction(action);
		setParam(param);
	}
	
	private int action;
	private Hashtable param;
	
	public int getAction()
	{
		return action;
	}
	
	public void setAction(int action)
	{
		this.action = action;
	}
	
	public void setParam(Hashtable param)
	{
		this.param = param;
	}
	
	public Hashtable getParam()
	{
		return param;
	}
}
