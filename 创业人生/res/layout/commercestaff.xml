<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:clickable="true"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  >
   <ImageView 
   android:id="@+id/imageview"
   android:layout_width="fill_parent"
   android:layout_height="fill_parent"
   android:background="@drawable/selectsever_backgroud" 
    />
 <Button
   android:id="@+id/commercestaff_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/returnback_selector"
  />
  <Button
   android:id="@+id/commercestaff_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/commercestaff_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:text="商会"
  	android:textColor="@android:color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_toRightOf="@id/commercestaff_returnback"
  	android:layout_toLeftOf="@id/commercestaff_cancel"
  />  
  <ImageView
  android:id="@+id/commercestaff_smallback"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/commercestaff_title"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_marginTop="5px"
  android:layout_marginLeft="50px"
  android:layout_marginRight="50px"
  android:layout_marginBottom="20px"
  android:background="@drawable/selectsever_smallback"
  android:focusable="false"
  />  
  <TextView 
  android:id="@+id/staffname"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/commercestaff_title"
  android:layout_marginTop="20px"
  android:layout_alignLeft="@id/commercestaff_smallback"
  android:layout_marginLeft="45px"
  android:text="姓名"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/companyrate"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/commercestaff_title"
  android:layout_marginTop="20px"
  android:layout_toLeftOf="@+id/staffsex"
  android:layout_marginRight="100px"
  android:text="公司等级"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/staffsex"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/commercestaff_title"
  android:layout_toLeftOf="@+id/staffposition"
  android:layout_marginTop="20px"
  android:layout_marginRight="100px"
  android:text="性别"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  
  <TextView 
  android:id="@+id/staffposition"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/commercestaff_title"
  android:layout_alignRight="@+id/commercestaff_smallback"
  android:layout_marginTop="20px"
  android:layout_marginRight="50px"
  android:text="职务"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/line"
  android:layout_width="wrap_content"
  android:layout_height="2px"
  android:layout_alignLeft="@+id/commercestaff_smallback"
  android:layout_alignRight="@+id/commercestaff_smallback"
  android:layout_below="@+id/staffsex"
  android:layout_marginTop="5px"
  android:background="@drawable/line"
  />
      <LinearLayout
  android:id="@+id/commercestaff_buttonlayout"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignLeft="@+id/commercestaff_smallback"
  android:layout_alignRight="@+id/commercestaff_smallback"
  android:layout_alignBottom="@+id/commercestaff_smallback"
  >
  
  <RelativeLayout
  android:id="@+id/commercestaff_button_info_layout"
  android:layout_width="0px"
  android:layout_weight="1"
  android:layout_height="wrap_content"
  >
  <Button
  android:id="@+id/commercestaff_button_info"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/button_information"
  />
  </RelativeLayout>
  
  
   <RelativeLayout
  android:id="@+id/commercestaff_button_uper_layout"
  android:layout_width="0px"
  android:layout_weight="1"
  android:layout_height="wrap_content"
  >
   <Button
  android:id="@+id/commercestaff_button_uper"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/button_uper"
  />
  </RelativeLayout>
  
  <RelativeLayout
  android:id="@+id/commercestaff_button_member_layout"
  android:layout_width="0px"
  android:layout_weight="1"
  android:layout_height="wrap_content"
  >
   <Button
  android:id="@+id/commercestaff_button_member"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/button_staff_pressed"
  />
  </RelativeLayout>
  
  <RelativeLayout
  android:id="@+id/commercestaff_button_diary_layout"
  android:layout_width="0px"
  android:layout_weight="1"
  android:layout_height="wrap_content"
  >
   <Button
  android:id="@+id/commercestaff_button_diary"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/button_diary"
  />
  </RelativeLayout>
  
  
  </LinearLayout>
  <ListView 
  android:id="@+id/commercestafflistview"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_below="@+id/line"
  android:layout_above="@+id/commercestaff_buttonlayout"
 
  android:layout_alignLeft="@+id/commercestaff_smallback"
  android:layout_alignRight="@+id/commercestaff_smallback"
  
  
  android:layout_marginTop="5px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="5px"
  android:layout_marginBottom="10px"
  android:scrollbars="none"
  android:focusable="false"
  android:cacheColorHint="#00000000"
   >
   </ListView>
 
    
    
</RelativeLayout>