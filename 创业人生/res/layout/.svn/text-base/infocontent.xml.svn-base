<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
   android:clickable="true"
  android:background="@drawable/selectsever_backgroud"
  >
  
  
   <Button
   android:id="@+id/infocontent_button_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/returnback_selector"
  />
  <Button
   android:id="@+id/infocontent_button_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/infocontent_button_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:textColor="@color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_toRightOf="@id/infocontent_button_returnback"
  	android:layout_toLeftOf="@id/infocontent_button_cancel"
  /> 
  
  
   <ImageView
  android:id="@+id/infocontent_scrollviewback"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_below="@id/infocontent_button_title"
   
  android:layout_marginTop="5px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="10px"
  android:layout_marginBottom="10px"
  
  android:layout_alignLeft="@id/infocontent_button_returnback"
   android:layout_alignRight="@id/infocontent_button_cancel"
  
  android:background="@drawable/selectsever_smallback"
  />
  <ScrollView
  		android:id="@+id/infocontent_scroll"
  		android:layout_height="wrap_content"
  		android:layout_width="wrap_content"
  		android:layout_alignTop="@id/infocontent_scrollviewback"
  	 	android:layout_alignLeft="@id/infocontent_scrollviewback"
   		android:layout_alignRight="@id/infocontent_scrollviewback"
   		android:layout_alignBottom="@id/infocontent_scrollviewback"
   		>
   		<LinearLayout android:id="@+id/infocontent_scroll_linear"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content">
   			<TextView android:id="@+id/infocontent_scroll_linear_tv"
   				android:layout_height="wrap_content"
   				android:layout_width="fill_parent"
   				android:textColor="@color/black"
   				android:textSize="@dimen/selectsever_selectsever_item_textsize"/>	
   		</LinearLayout>  		
  </ScrollView>

</RelativeLayout>
