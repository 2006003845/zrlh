<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/sj_middle_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/image_0" />

    <ImageButton
        android:id="@+id/sj_middle_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dip"
        android:background="@drawable/image_1"
        android:src="@drawable/image_89" />

    <Button
        android:id="@+id/sj_middle_exit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/middle_back"
        android:layout_alignTop="@id/middle_back"
        android:layout_marginLeft="30dip"
        android:layout_marginTop="15dip"
        android:background="@drawable/exit_0_pressed" />

    <ImageView
        android:id="@+id/sj_imageyan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/sj_middle_back"
        android:layout_alignTop="@id/sj_middle_back"
        android:layout_marginRight="20dip"
        android:layout_marginTop="15dip"
        android:background="@drawable/image_75" />

    <Button
        android:id="@+id/sj_middle_confirme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/sj_middle_back"
        android:layout_alignTop="@id/sj_middle_back"
        android:layout_marginRight="30dip"
        android:layout_marginTop="30dip"
        android:background="@drawable/confirme_0_pressed" />

    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignBottom="@id/sj_middle_back"
        android:layout_alignLeft="@id/sj_middle_back"
        android:layout_alignRight="@id/sj_middle_back"
        android:layout_below="@id/sj_middle_name"
        android:layout_marginBottom="20dip"
        android:layout_marginLeft="85dip"
        android:layout_marginRight="75dip"
        android:layout_marginTop="5dip"
        android:orientation="vertical">
        
        <ImageView 
            android:id="@+id/sj_image1"
            android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_gravity="center_horizontal"
        	android:background="@drawable/image_90"/>
        <RelativeLayout 
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content">
            <ImageView 
                android:id="@+id/sj_image2"
           		android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:background="@drawable/image_2"/>

            <ScrollView
                android:id="@+id/scrollview1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"               
  				android:scrollbars="none"
                android:layout_alignBottom="@id/sj_image2"
                android:layout_alignLeft="@id/sj_image2"
                android:layout_alignRight="@id/sj_image2"
                android:layout_alignTop="@id/sj_image2"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip" >

                <TextView
                    android:id="@+id/sj_text1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textSize="20sp" />
            </ScrollView>

        </RelativeLayout>
        <ImageView 
            android:id="@+id/sj_image3"
            android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_gravity="center_horizontal"
        	android:background="@drawable/image_91"/>
        <RelativeLayout 
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content">
            <ImageView 
                android:id="@+id/sj_image4"
           		android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:background="@drawable/image_2"/>
            <ScrollView
                android:id="@+id/scrollview2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"               
  				android:scrollbars="none"
                android:layout_alignBottom="@id/sj_image4"
                android:layout_alignLeft="@id/sj_image4"
                android:layout_alignRight="@id/sj_image4"
                android:layout_alignTop="@id/sj_image4"
                android:layout_marginLeft="10dip"
                android:layout_marginRight="10dip" >

                <TextView
                    android:id="@+id/sj_text2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textSize="20sp" />
            </ScrollView>
        </RelativeLayout>
        
    </LinearLayout>


</RelativeLayout>