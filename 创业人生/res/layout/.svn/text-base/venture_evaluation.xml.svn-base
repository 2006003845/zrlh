<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
   android:clickable="true"
  android:background="@drawable/selectsever_backgroud"
  >
  
  
   <Button
   android:id="@+id/venture_evaluation_button_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/returnback_selector"
  />
  <Button
   android:id="@+id/venture_evaluation_button_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/venture_evaluation_button_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:text="创业测评"
  	android:textColor="@color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_toRightOf="@id/venture_evaluation_button_returnback"
  	android:layout_toLeftOf="@id/venture_evaluation_button_cancel"
  /> 
  
  
   <ImageView
  android:id="@+id/venture_evaluation_scrollviewback"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_below="@id/venture_evaluation_button_title"
   
  android:layout_marginTop="5px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="10px"
  android:layout_marginBottom="10px"
  
  android:layout_alignLeft="@id/venture_evaluation_button_returnback"
  android:layout_alignRight="@id/venture_evaluation_button_cancel"
  android:background="@drawable/selectsever_smallback"
  />
  
  <RelativeLayout android:id="@+id/venture_evaluation_relative1"
  	android:layout_height="wrap_content"
  	android:layout_width="wrap_content"
	android:background="@drawable/policyinfolist_frame3"
  	android:layout_alignLeft="@id/venture_evaluation_scrollviewback"
  	android:layout_alignTop="@id/venture_evaluation_scrollviewback"
  	android:layout_alignRight="@id/venture_evaluation_scrollviewback">
  	<TextView android:id="@+id/venture_evalution_tv1"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_centerVertical="true"
  		android:singleLine="true"
  		android:textColor="@color/black"
  		android:textSize="@dimen/selectsever_selectsever_item_textsize"
  		android:layout_marginLeft="20px"
  		/>
  	<ImageView android:id="@+id/venture_evalutaion_img1"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_alignParentRight="true"
  		android:layout_marginRight="40px"
  		android:src="@drawable/venture_evalution_autognosis"
  		/>
  </RelativeLayout>
  
  <RelativeLayout android:id="@+id/venture_evaluation_relative2"
  	android:layout_height="wrap_content"
  	android:layout_width="wrap_content"
	android:background="@drawable/policyinfolist_frame3"
  	android:layout_alignLeft="@id/venture_evaluation_scrollviewback"
  	android:layout_below="@id/venture_evaluation_relative1"
  	android:layout_alignRight="@id/venture_evaluation_scrollviewback">
  	<TextView android:id="@+id/venture_evalution_tv2"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:textColor="@color/black"
  		android:layout_centerVertical="true"
  		android:singleLine="true"
  		android:textSize="@dimen/selectsever_selectsever_item_textsize"
  		android:layout_marginLeft="20px"
  		/>
  	<ImageView android:id="@+id/venture_evalutaion_img2"
  		android:layout_width="wrap_content"
  		android:layout_height="wrap_content"
  		android:layout_alignParentRight="true"
  		android:layout_marginRight="40px"
  		android:src="@drawable/venture_evalution_potential"
  		/>
  </RelativeLayout>

</RelativeLayout>
