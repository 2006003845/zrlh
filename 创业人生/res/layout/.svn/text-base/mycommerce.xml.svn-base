<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:clickable="true"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  >
   <ImageView 
   android:id="@+id/imageview"
   android:layout_width="fill_parent"
   android:layout_height="fill_parent"
   android:background="@drawable/selectsever_backgroud" 
    />
 <Button
   android:id="@+id/commerceinfo_info"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:text="信息"
   android:textColor="@color/white"
   android:textSize="18sp"
   android:background="@drawable/selection_selected"
  />
  <Button
   android:id="@+id/commerceinfo_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/commerceinfo_dairy"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_toRightOf="@id/commerceinfo_info"
  	android:layout_marginLeft="5px"
  	android:layout_marginTop="5px"
  	android:text="日志"
  	android:textColor="@color/white"
  	android:textSize="18sp"
  	android:background="@drawable/selection"
  />  
  <Button
    android:id="@+id/commerceinfo_staff"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_toRightOf="@id/commerceinfo_dairy"
  	android:layout_marginLeft="5px"
  	android:layout_marginTop="5px"
  	android:text="成员"
  	android:textColor="@color/white"
  	android:textSize="18sp"
  	android:background="@drawable/selection"
  />  
  <Button
    android:id="@+id/commerceinfo_propaganda"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_toRightOf="@id/commerceinfo_staff"
  	android:layout_marginLeft="5px"
  	android:layout_marginTop="5px"
  	android:text="联合宣传"
  	android:textColor="@color/white"
  	android:textSize="18sp"
  	android:background="@drawable/selection"
  />  
  <Button
    android:id="@+id/commerceinfo_commercelist"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_toRightOf="@id/commerceinfo_propaganda"
  	android:layout_marginLeft="5px"
  	android:layout_marginTop="5px"
  	android:text="商会列表"
  	android:textColor="@color/white"
  	android:textSize="18sp"
  	android:background="@drawable/selection"
  />  
  <ImageView
  android:id="@+id/commerceinfo_smallback"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/commerceinfo_info"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_marginTop="5px"
  android:layout_marginLeft="50px"
  android:layout_marginRight="50px"
  android:layout_marginBottom="20px"
  android:background="@drawable/selectsever_smallback"
  android:focusable="false"
  /> 
   <ViewFlipper android:id="@+id/flipper"   
       android:layout_width="wrap_content"     
       android:layout_height="wrap_content" 
       android:layout_alignLeft="@id/commerceinfo_smallback"  
       android:layout_alignRight="@id/commerceinfo_smallback"  
       android:layout_alignTop="@id/commerceinfo_smallback"  
       android:layout_alignBottom="@id/commerceinfo_smallback"  
       android:persistentDrawingCache="animation"   
       android:flipInterval="1000"   
       android:inAnimation="@anim/push_left_in"   
       android:outAnimation="@anim/push_left_out"> 
  
  <RelativeLayout
      android:id="@+id/relative_flipper1"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
    >
       
  <Button 
      android:id="@+id/button_invisible"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="5px"
	  android:layout_marginLeft="5px"
      android:visibility="invisible"
      />
  <ImageView 
  android:id="@+id/commerceinfo_logo"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_toRightOf="@id/button_invisible"
  android:layout_marginLeft="10px"
  android:layout_marginTop="5px"
 
  />
    <TextView 
  android:id="@+id/commerceinfo_line"
  android:layout_width="fill_parent"
  android:layout_height="2px"
  android:layout_below="@id/commerceinfo_logo"
  android:layout_centerHorizontal="true"
  android:layout_marginTop="5px"
  android:background="@drawable/line"
  />
    <ImageView 
  android:id="@+id/notice_detail"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@id/commerceinfo_info"
  android:layout_above="@id/commerceinfo_line"
  android:layout_toRightOf="@id/commerceinfo_logo"
  android:layout_marginTop="5px"
  android:layout_marginLeft="10px"
  android:layout_marginRight="10px"
  android:layout_marginBottom="5px"
  android:background="@drawable/selectsever_smallback"
  android:focusable="false"
  />
  <TextView 
  android:id="@+id/notice_text"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignLeft="@+id/notice_detail"
  android:layout_alignTop="@+id/notice_detail"
  android:layout_alignRight="@+id/notice_detail"
  android:layout_alignBottom="@+id/notice_detail"
  android:gravity="center"
  android:singleLine="false"   
  android:scrollbars="vertical" 
  android:textColor="@android:color/black"
  android:textSize="18sp"
  
  />
  <Button 
      android:id="@+id/commerce_invite"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="邀请会员"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />
  <Button 
      android:id="@+id/commerce_operate"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_toRightOf="@id/commerce_invite"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="商会操作"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />
  <!--  
  <Button 
      android:id="@+id/commerce_interactive"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_toRightOf="@id/commerce_operate"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="交互"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />
  -->
    <ListView 
  android:id="@+id/commerceuperlistview"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/commerceinfo_line"
  android:layout_above="@id/commerce_invite" 
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_marginTop="10px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="5px"
  android:layout_marginBottom="10px"
  android:scrollbars="none"
  android:focusable="false"
  android:cacheColorHint="#00000000"
   >
   </ListView>
   </RelativeLayout>
   <RelativeLayout 
      android:id="@+id/relative_flipper2"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
       >
      <TextView 
  android:id="@+id/commercediarytextview"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_alignParentTop="true"
  android:layout_alignParentBottom="true"
  android:textColor="@color/black"
  android:textSize="20dp"
  android:layout_marginTop="10px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="5px"
  android:layout_marginBottom="10px"
  android:scrollbars="none"
  android:focusable="false" >
   </TextView>   
       
   </RelativeLayout>
   
   <RelativeLayout 
      android:id="@+id/relative_flipper3"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
       >
 <TextView 
  android:id="@+id/staffname"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_marginTop="5px"
  android:layout_marginLeft="45px"
  android:text="姓名"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/companyrate"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_marginTop="5px"
  android:layout_toLeftOf="@+id/staffsex"
  android:layout_marginRight="100px"
  android:text="公司等级"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/staffsex"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_toLeftOf="@+id/staffposition"
  android:layout_marginTop="5px"
  android:layout_marginRight="100px"
  android:text="性别"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  
  <TextView 
  android:id="@+id/staffposition"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignParentRight="true"
  android:layout_marginTop="5px"
  android:layout_marginRight="50px"
  android:text="职务"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/line"
  android:layout_width="wrap_content"
  android:layout_height="2px"
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_below="@+id/staffsex"
  android:layout_marginTop="5px"
  android:background="@drawable/line"
  />  
    <Button 
      android:id="@+id/commerce_check"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="查看此人"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />
  <Button 
      android:id="@+id/commerce_interactive"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_toRightOf="@id/commerce_check"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="交互"
      android:textColor="@color/white"
      android:textSize="18sp"
      android:background="@drawable/button_frame"
     
      />  
  <ListView 
  android:id="@+id/commercestafflistview"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/line"
  android:layout_above="@id/commerce_check"
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_marginTop="10px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="5px"
  android:layout_marginBottom="10px"
  android:scrollbars="none"
  android:focusable="false"
  android:cacheColorHint="#00000000"
   >
   </ListView>
    
       
   </RelativeLayout>
   
   <RelativeLayout 
      android:id="@+id/relative_flipper4"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
       >
    <Button 
      android:id="@+id/commerce_joinpropa"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignParentLeft="true"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="参加宣传"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />
  <Button 
      android:id="@+id/commerce_holdpropa"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_toRightOf="@id/commerce_joinpropa"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="发起宣传"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />  
  <Button 
      android:id="@+id/commerce_checkpropa"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_toRightOf="@id/commerce_holdpropa"
      android:layout_alignParentBottom="true"
      android:layout_marginLeft="5px"
      android:text="查看此人"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:background="@drawable/button_frame"
     
      />  
 <ListView
  android:id="@+id/propagandalist_listview"
  android:layout_centerHorizontal="true"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_alignParentTop="true"
  android:layout_alignParentBottom="true"
  android:layout_marginTop="10px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="5px"
  android:layout_marginBottom="10px"
  android:focusable="false"
  android:cacheColorHint="#00000000"
  >
   
  </ListView>
    
       
   </RelativeLayout>
   <RelativeLayout      
      android:id="@+id/relative_flipper5"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent" 
     >
       <TextView 
  android:id="@+id/commercename"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_marginTop="5px"
  android:layout_marginLeft="45px"
  android:text="商会列表"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/commercerate"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_marginTop="5px"
  
  android:layout_centerHorizontal="true"
  android:text="等级"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/commercenumber"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignParentRight="true"
  android:layout_marginTop="5px"
  android:layout_marginRight="100px"
  android:text="人数"
  android:textColor="@android:color/black"
  android:textSize="20dp"
  />
  <TextView 
  android:id="@+id/line"
  android:layout_width="wrap_content"
  android:layout_height="2px"
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_below="@id/commercenumber"
  android:layout_marginTop="5px"
  android:background="@drawable/line"
  />
    <ListView 
  android:id="@+id/commercelistview"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/line"
  android:layout_alignParentLeft="true"
  android:layout_alignParentRight="true"
  android:layout_alignParentBottom="true"

  
  android:layout_marginTop="5px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="5px"
  android:layout_marginBottom="10px"
  android:scrollbars="none"
  android:focusable="false"
  android:cacheColorHint="#00000000"
   >
   </ListView>  
   </RelativeLayout>
</ViewFlipper>
</RelativeLayout>
