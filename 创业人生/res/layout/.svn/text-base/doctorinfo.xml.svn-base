<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:clickable="true"
  android:background="@drawable/selectsever_backgroud"
  >
 <Button 
 android:id="@+id/doctor_button_returnback"
 android:layout_width="wrap_content"
 android:layout_height="wrap_content"
 android:layout_marginLeft="5px"
 android:layout_marginTop="5px"
 android:background="@drawable/returnback_selector"
  />   
 <Button 
 android:id="@+id/doctor_button_cancel"
 android:layout_width="wrap_content"
 android:layout_height="wrap_content"
 android:layout_alignParentRight="true"
 android:layout_marginRight="5px"
 android:layout_marginTop="5px"
 android:background="@drawable/cancel_selector"
  />   
 <Button 
 android:id="@+id/doctor_button_title"
 android:layout_width="wrap_content"
 android:layout_height="wrap_content"
 android:layout_centerHorizontal="true"
 android:gravity="center_vertical|center_horizontal"
 android:layout_toLeftOf="@+id/doctor_button_cancel"
 android:layout_toRightOf="@+id/doctor_button_returnback"
 android:background="@drawable/selectsever_titleback"
 android:text="睿博士"
 android:textSize="30dp"
 android:textColor="@color/black"
 android:focusable="false"
  />   
  <ImageView
  android:id="@+id/doctor_smallback"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/doctor_button_title"
 android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_marginTop="5px"
  android:layout_marginLeft="20px"
  android:layout_marginRight="20px"
  android:layout_marginBottom="10px"
  android:background="@drawable/selectsever_smallback"
  android:focusable="false"
  />
  <ImageView 
  android:id="@+id/doctor_imageview_logo"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/doctor_button_title"
  android:layout_marginTop="10px"
  android:layout_centerHorizontal="true"
  android:background="@drawable/doctor_logo"
   />
  <Button 
  android:id="@+id/doctor_button_upgrade"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/doctor_button_title"
  android:layout_marginTop="30px"
  android:layout_alignRight="@id/doctor_smallback"
  android:layout_marginRight="10px"
  android:background="@drawable/doctor_upgrade"
  />
  <TextView 
  android:id="@+id/doctor_line"
  android:layout_width="wrap_content"
  android:layout_height="2px"
  android:layout_alignLeft="@+id/doctor_smallback"
  android:layout_alignRight="@+id/doctor_smallback"
  android:layout_below="@+id/doctor_imageview_logo"
  android:layout_marginTop="5px"
  android:background="@drawable/line"
  />
  <LinearLayout 
  android:id="@+id/doctor_buttonlayout"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignLeft="@+id/doctor_smallback"
  android:layout_alignRight="@+id/doctor_smallback"
  android:layout_alignBottom="@+id/doctor_smallback"
  >
  <RelativeLayout 
  android:id="@+id/doctor_button_info_layout"
  android:layout_width="0px"
  android:layout_height="wrap_content"
  android:layout_weight="1"
  >
  <Button 
  android:id="@+id/doctor_button_info"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/doctor_info_pressed"
  />
  </RelativeLayout>
  
  <RelativeLayout 
  android:id="@+id/doctor_button_custody_layout"
  android:layout_width="0px"
  android:layout_height="wrap_content"
  android:layout_weight="1"
  >
  <Button 
  android:id="@+id/doctor_button_custody"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/doctor_custody"
  />
  </RelativeLayout>
  
  <RelativeLayout 
  android:id="@+id/doctor_button_task_layout"
  android:layout_width="0px"
  android:layout_height="wrap_content"
  android:layout_weight="1"
  >
  <Button 
  android:id="@+id/doctor_button_task"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/doctor_task"
  />
  </RelativeLayout>
  
  <RelativeLayout 
  android:id="@+id/doctor_button_shop_layout"
  android:layout_width="0px"
  android:layout_height="wrap_content"
  android:layout_weight="1"
  >
  <Button 
  android:id="@+id/doctor_button_shop"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_centerInParent="true"
  android:background="@drawable/doctor_shop"
  />
  </RelativeLayout>
  
  </LinearLayout>
  <TextView 
  android:id="@+id/doctor_text_level"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignLeft="@id/doctor_smallback"
  android:layout_marginLeft="50px"
  android:layout_below="@+id/doctor_line"
  android:layout_marginTop="10px"
  android:text="等级"
  android:textColor="@color/black"
  android:textSize="20dp"
  
  />
  <Button 
  android:id="@+id/doctor_button_level"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/doctor_line"
  android:layout_marginTop="10px"
  android:layout_toRightOf="@+id/doctor_text_level"
  android:layout_marginLeft="50px"
  android:text="10"
  android:textColor="@color/black"
  android:textSize="20dp"
  android:background="@drawable/colorbar_white"
  />
    <TextView 
  android:id="@+id/doctor_text_points"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_toRightOf="@+id/doctor_button_level"
  android:layout_marginLeft="80px"
  android:layout_below="@+id/doctor_line"
  android:layout_marginTop="10px"
  android:text="创业点数"
  android:textColor="@color/black"
  android:textSize="20dp"
  
  />
    <Button 
  android:id="@+id/doctor_button_points"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/doctor_line"
  android:layout_marginTop="10px"
  android:layout_toRightOf="@+id/doctor_text_points"
  android:layout_marginLeft="50px"
  android:text="100000"
  android:textColor="@color/black"
  android:textSize="20dp"
  android:background="@drawable/colorbar_white"
  />
    <TextView 
  android:id="@+id/doctor_text_strength"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignLeft="@id/doctor_smallback"
  android:layout_marginLeft="50px"
  android:layout_below="@+id/doctor_text_level"
  android:layout_marginTop="20px"
  android:text="体力"
  android:textColor="@color/black"
  android:textSize="20dp"
  />
    <Button 
  android:id="@+id/doctor_button_strength"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/doctor_button_level"
  android:layout_marginTop="20px"
  android:layout_toRightOf="@+id/doctor_text_strength"
  android:layout_marginLeft="50px"
  android:text="50/50"
  android:textColor="@color/black"
  android:textSize="20dp"
  android:background="@drawable/colorbar_red"
  />
    <TextView 
  android:id="@+id/doctor_text_shopincome"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_toRightOf="@+id/doctor_button_strength"
  android:layout_marginLeft="80px"
  android:layout_below="@+id/doctor_text_points"
  android:layout_marginTop="20px"
  android:text="店铺收益"
  android:textColor="@color/black"
  android:textSize="20dp"
  
  />
      <Button 
  android:id="@+id/doctor_button_shopincome"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_below="@+id/doctor_button_points"
  android:layout_marginTop="20px"
  android:layout_toRightOf="@+id/doctor_text_shopincome"
  android:layout_marginLeft="50px"
  android:text="+50%"
  android:textColor="@color/black"
  android:textSize="20dp"
  android:background="@drawable/colorbar_white"
  />
</RelativeLayout>
