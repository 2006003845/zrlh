<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:background="@drawable/selectsever_backgroud"
  android:clickable="true"
  >
  
  <Button
   android:id="@+id/shopinfo_button_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/returnback_selector"
  />
  <Button
   android:id="@+id/shopinfo_button_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/shopinfo_button_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:text="店铺信息"
  	android:textColor="@color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_toRightOf="@id/shopinfo_button_returnback"
  	android:layout_toLeftOf="@id/shopinfo_button_cancel"
  />
  
  <ImageView
  android:id="@+id/shopinfo_shopImageview"
  android:layout_width="250px"
  android:layout_height="250px" 
  android:layout_marginLeft="5px"
  android:layout_centerVertical="true"
  
  />
  
  <ImageView
  android:id="@+id/shopinfo_scrollviewback"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_below="@id/shopinfo_button_title"
  android:layout_toRightOf="@id/shopinfo_shopImageview" 
  android:layout_marginTop="5px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="10px"
  android:layout_marginBottom="10px"
  android:background="@drawable/selectsever_smallback"
  />
  
  
  
  <ScrollView
  android:id="@+id/shopinfo_scrollview"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:scrollbars="none"
  android:layout_alignLeft="@id/shopinfo_scrollviewback"
  android:layout_alignRight="@id/shopinfo_scrollviewback"
  android:layout_alignTop="@id/shopinfo_scrollviewback"
  android:layout_alignBottom="@id/shopinfo_scrollviewback"
  >
  <LinearLayout
  android:id="@+id/shopinfo_layout"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:orientation="vertical"
 
  >
   
  <ImageView
  android:id="@+id/shopinfo_parting_line0"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:background="@drawable/parting_line"
  />
  <TextView
  android:id="@+id/shopinfo_income"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="收入:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />
  <ImageView
  android:id="@+id/shopinfo_parting_line1"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <TextView
  android:id="@+id/shopinfo_bonus"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="加成:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />
  <ImageView
  android:id="@+id/shopinfo_parting_line2"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <TextView
  android:id="@+id/shopinfo_shopname"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店铺名称:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />
  <ImageView
  android:id="@+id/shopinfo_parting_line3"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
   <RelativeLayout
  android:id="@+id/shopinfo_layout3"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  >
  


<TextView
  android:id="@+id/shopinfo_coordinates"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店铺坐标:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />

    <Button
  android:id="@+id/shopinfo_strategy_button"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignParentRight="true"
  />
  
  
  <Button
  android:id="@+id/shopinfo_see_button"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:background="@drawable/button_see"
  android:layout_alignParentRight="true"
  />
  </RelativeLayout>

  <ImageView
  android:id="@+id/shopinfo_parting_line4"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <RelativeLayout
  android:id="@+id/shopinfo_layout"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  >
   <TextView
  android:id="@+id/shopinfo_flow"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店铺客流:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />
  <Button
  android:id="@+id/shopinfo_flow_button"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignParentRight="true"
  />
  
  </RelativeLayout>
 
  <ImageView
  android:id="@+id/shopinfo_parting_line5"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <RelativeLayout
  	android:id="@+id/shopinfo_layout2"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
  >
  	<TextView
	  android:id="@+id/shopinfo_inventory"
 	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:text="店铺库存:"
	  android:textSize="20sp"
 	 android:textColor="@color/black"
	  />
	
	  <Button
	  android:id="@+id/shopinfo_purchase_button"
	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:layout_alignParentRight="true"
	  android:background="@drawable/button_purchase"
  />
  
  <com.zrong.Android.View.SpriteView
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:id="@+id/shopinfo_buy_arrows"
		android:layout_toLeftOf="@id/shopinfo_purchase_button"
		resID="1391" 
		android:visibility="gone"/>
  
  </RelativeLayout>
  
  <ImageView
  android:id="@+id/shopinfo_parting_line6"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <RelativeLayout
  android:id="@+id/shopinfo_layout1"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  
  >
   <TextView
  android:id="@+id/shopinfo_employees"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店长:"
 android:textSize="20sp"
  android:textColor="@color/black"
  />
  <Button
  android:id="@+id/shopinfo_employees_button"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignParentRight="true"
  />
   <com.zrong.Android.View.SpriteView
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:id="@+id/shopinfo_employee_arrows"
		android:layout_toLeftOf="@id/shopinfo_employees_button" 
		resID="1391" 
		android:visibility="gone"/>
 </RelativeLayout>
  <ImageView
  android:id="@+id/shopinfo_parting_line7"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <TextView
  android:id="@+id/shopinfo_quality"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店铺品质:"
  android:textSize="20sp"
  android:textColor="@color/black" 
  />
  <ImageView
  android:id="@+id/shopinfo_parting_line8"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <TextView
  android:id="@+id/shopinfo_popularity"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店铺知名度:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />
  <ImageView
  android:id="@+id/shopinfo_parting_line9"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
   android:background="@drawable/parting_line"
  />
  <TextView
  android:id="@+id/shopinfo_morale"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:text="店铺士气:"
  android:textSize="20sp"
  android:textColor="@color/black"
  />

  </LinearLayout>
  </ScrollView>	  	

  
  <LinearLayout android:id="@+id/linearlayout_promotframe4"
   		android:layout_below="@id/shopinfo_button_title"
   		android:visibility="gone" 
   		android:orientation="horizontal"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content"
		android:background="@drawable/promotframe"
		android:layout_centerHorizontal="true"
		>
		<TextView android:id="@+id/promotFrame4" 
			android:layout_width="wrap_content"
			android:gravity="center" android:textColor="#000000"
			android:layout_gravity="center" android:textSize="20sp"
			android:layout_marginRight="10sp" android:layout_height="wrap_content" />
		<Button android:id="@+id/close_promotframe4" 
			android:background="@drawable/cancel_selector"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_alignParentRight="true" 
			android:layout_marginTop="22sp"
			android:layout_marginLeft="20sp" />
	</LinearLayout>
	
	   
		
</RelativeLayout>
