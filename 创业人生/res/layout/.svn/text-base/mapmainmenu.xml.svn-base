<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:clickable="true"
  android:background="@drawable/selectsever_backgroud"
  >
  
  <Button
   android:id="@+id/mapmainmenu_button_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/returnback_selector"
  />
  <Button
   android:id="@+id/mapmainmenu_button_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/mapmainmenu_button_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:text="主菜单"
  	android:textColor="@color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_toRightOf="@id/mapmainmenu_button_returnback"
  	android:layout_toLeftOf="@id/mapmainmenu_button_cancel"
  /> 
   <ImageView
  android:id="@+id/mapmainmenu_smallback"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/mapmainmenu_button_title"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_marginTop="5px"
  android:layout_marginLeft="50px"
  android:layout_marginRight="50px"
  android:layout_marginBottom="50px"
  android:background="@drawable/selectsever_smallback"
  android:focusable="false"
  />
  
  <ListView
  android:id="@+id/mapmainmenu_listview"
  android:layout_centerHorizontal="true"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:layout_alignTop="@id/mapmainmenu_smallback"
  android:layout_alignLeft="@id/mapmainmenu_smallback"
  android:layout_alignRight="@id/mapmainmenu_smallback"
  android:layout_alignBottom="@id/mapmainmenu_smallback"
  
  android:layout_marginTop="10px"
  android:layout_marginLeft="10px"
  android:layout_marginRight="10px"
  android:layout_marginBottom="10px"
  android:focusable="false"
  />
	  
		<com.zrong.Android.View.SpriteView
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:id="@+id/mapmainmenu_shop_arrows"
		android:layout_alignTop="@id/mapmainmenu_smallback"
		android:layout_marginTop="55dip"
		android:layout_marginRight="20dip"
		resID="1391"
		android:visibility="invisible"/>
		
		  <com.zrong.Android.View.SpriteView
		android:layout_height="wrap_content"
		android:layout_width="wrap_content"
		android:id="@+id/mapmainmenu_employee_arrows"
		android:layout_below="@id/mapmainmenu_shop_arrows"
		android:layout_marginRight="3dip"
		resID="1391" 
		android:visibility="invisible"/>
   
</RelativeLayout>
