<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
   android:clickable="true"
  android:background="@drawable/selectsever_backgroud"
  >
  <Button
    android:id="@+id/characterdetail_button_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:text="常务"
  	android:textColor="@color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_marginTop="5px"

  /> 
   <ImageView
  android:id="@+id/characterdetail_smallback"
  android:layout_centerHorizontal="true"
  android:layout_below="@id/characterdetail_button_title"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_marginTop="5px"
  android:layout_marginLeft="20px"
  android:layout_marginRight="20px"
  android:layout_marginBottom="10px"
  android:background="@drawable/selectsever_smallback"
  android:focusable="false"
  />
   <!--  
   <Button
   android:id="@+id/employeelist_routin_button_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/venture_card"
  />
  --> 
  <Button
   android:id="@+id/employeelist_routin_button_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
      <Button 
      android:id="@+id/confirm"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignRight="@id/characterdetail_smallback"
      android:layout_alignBottom="@+id/characterdetail_smallback"
      android:background="@drawable/button_frame"
      android:text="确认"
      android:textColor="@color/white"
      android:textSize="18sp"
      />
      <Button 
      android:id="@+id/confirm1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignLeft="@id/characterdetail_smallback"
      android:layout_alignBottom="@+id/characterdetail_smallback"
      android:background="@drawable/button_frame"
      android:text="确认"
      android:textColor="@color/white"
      android:textSize="18sp"
      android:visibility="invisible"
      />
  <!-- 
  <Button 
    android:id="@+id/characterdetail_button_card"  
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignRight="@id/characterdetail_smallback"  
    android:layout_alignBottom="@+id/characterdetail_smallback"  
    android:background="@drawable/button_card"
    />
     -->
       <TextView 
      android:id="@+id/routin_desc"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_centerHorizontal="true"
	  android:layout_toLeftOf="@id/confirm"  
	  android:layout_toRightOf="@id/confirm1"   
      android:layout_marginLeft="40dp"
      android:layout_marginRight="40dp"
      android:layout_alignBottom="@id/characterdetail_smallback"
      android:layout_marginBottom="20dp"
      android:gravity="center"
      android:background="@drawable/store_text"
      />
          <ImageView 
    android:id="@+id/image_arrow"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_centerHorizontal="true"
    android:layout_above="@id/routin_desc"
    android:layout_marginBottom="10dp"
    android:background="@drawable/selected_arrow"
    />

    <Gallery 
  android:id="@+id/employeelist_routin_gridview"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_alignLeft="@id/characterdetail_smallback"
  android:layout_alignRight="@id/characterdetail_smallback"
  android:layout_alignTop="@id/characterdetail_smallback"
  android:layout_marginLeft="50px"
  android:layout_marginRight="50px"
  android:layout_marginTop="30px"
  android:spacing="20px"
  android:clickable="true"
  android:focusable="true"
  />
