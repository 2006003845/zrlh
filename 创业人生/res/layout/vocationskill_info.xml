<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
   android:clickable="true"
  android:background="@drawable/selectsever_backgroud"
  >
  
  
   <Button
   android:id="@+id/vocationskill_info_button_returnback"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginLeft="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/returnback_selector"
  />
  <Button
   android:id="@+id/vocationskill_info_button_cancel"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_alignParentRight="true"
   android:layout_marginRight="5px"
   android:layout_marginTop="5px"
   android:background="@drawable/cancel_selector"
  />
  <Button
    android:id="@+id/vocationskill_info_button_title"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
  	android:layout_centerHorizontal="true"
  	android:text="职业技能"
  	android:textColor="@color/black"
  	android:textSize="30dp"
  	android:gravity="center_horizontal|center_vertical"
  	android:background="@drawable/selectsever_titleback"
  	android:focusable="false"
  	android:layout_toRightOf="@id/vocationskill_info_button_returnback"
  	android:layout_toLeftOf="@id/vocationskill_info_button_cancel"
  /> 
  
  
   <ImageView
  android:id="@+id/vocationskill_info_scrollviewback"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent"
  android:layout_below="@id/vocationskill_info_button_title"
   
  android:layout_marginTop="5px"
  android:layout_marginLeft="5px"
  android:layout_marginRight="10px"
  android:layout_marginBottom="10px"
  
  android:layout_alignLeft="@id/vocationskill_info_button_returnback"
   android:layout_alignRight="@id/vocationskill_info_button_cancel"
  
  android:background="@drawable/selectsever_smallback"
  />
  
  <RelativeLayout 
  		android:id="@+id/vocationskill_info_relative"
  		android:layout_height="wrap_content"
  		android:layout_width="wrap_content"
  		android:layout_alignTop="@id/vocationskill_info_scrollviewback"
  	 	android:layout_alignLeft="@id/vocationskill_info_scrollviewback"
   		android:layout_alignRight="@id/vocationskill_info_scrollviewback"
   		android:background="@drawable/policyinfolist_frame3">
  		 <ImageView android:id="@+id/vocationskill_info_img"
  			android:layout_height="wrap_content"
  			android:layout_width="wrap_content"
  			android:layout_alignParentTop="true"
  			android:layout_marginTop="10px"
  			android:layout_centerHorizontal="true"
  			/>
  </RelativeLayout>
  <ScrollView android:id="@+id/vocationskill_info_scrollview"
  	android:layout_width="wrap_content"
  	android:layout_height="wrap_content"
   	android:layout_below="@id/vocationskill_info_relative"
   	android:layout_alignLeft="@id/vocationskill_info_scrollviewback"
   	android:layout_alignRight="@id/vocationskill_info_scrollviewback"
   	android:layout_alignBottom="@id/vocationskill_info_scrollviewback">
   	<LinearLayout android:id="@+id/vocationskill_info_scroll_linear"
   		android:layout_height="wrap_content"
   		android:layout_width="wrap_content">
   		<TextView android:id="@+id/vocationskill_info_scroll_linear_tv"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content"
   			android:layout_gravity="center"
   			android:hint="相关信息"
   			android:textColorHint="@color/black"
   			android:textSize="@dimen/selectsever_selectsever_item_textsize"
   			android:textColor="@color/black"/>
   		</LinearLayout>
  </ScrollView>

</RelativeLayout>


