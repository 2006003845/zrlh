    <Gallery 
  android:id="@+id/logo1_gallery"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:layout_centerHorizontal="true"
  android:layout_alignLeft="@id/characterdetail_smallback"
  android:layout_alignRight="@id/characterdetail_smallback"
  android:layout_alignTop="@id/characterdetail_smallback"
  android:layout_marginLeft="30px"
  android:layout_marginRight="30px"
  android:layout_marginTop="30px"
  android:spacing="20px"
  android:clickable="true"
  android:focusable="true"
  />
